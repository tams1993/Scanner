<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tams1993.scanner" >

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Main2Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Scanner.ScannerActivity"
            android:label="@string/title_activity_scanner" >
        </activity>
        <activity
            android:name=".generator.GenerateQRCodeActivity"
            android:label="@string/title_activity_generator" >
        </activity>
        <activity
            android:name=".Scanner.ExamPage"
            android:label="@string/title_activity_ecam_page" >
        </activity>

        <activity
            android:name="com.google.zxing.client.android.CaptureActivity"
            android:screenOrientation="landscape" />
    </application>

</manifest>
